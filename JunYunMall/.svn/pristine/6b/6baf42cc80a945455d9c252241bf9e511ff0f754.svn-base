<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f1f1f1"
    android:orientation="vertical"
    tools:context=".MyCollectActivity" >
 <include
        android:id="@+id/title"
        layout="@layout/layout_header" />
    <LinearLayout
        android:id="@+id/llyout_mybady_sort"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal" >
        <TextView
            android:id="@+id/tv_collect_mybady"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/shape_mybaby_tab"
            android:gravity="center"
            android:onClick="notSpending"
            android:textSize="14sp"
            android:text="宝贝"
            android:textColor="#898787" />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:background="#d7d7d7" />
        <TextView
            android:id="@+id/tv_collect_myshop"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/shape_mybaby_tab"
            android:gravity="center"
            android:onClick="yetSpwnt"
            android:textSize="14sp"
            android:text="店铺"
            android:textColor="#898787" />
    </LinearLayout>

    <ImageView
        android:id="@+id/img_tabLine"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:contentDescription="@null"
        android:scaleType="matrix" />
    <View
        android:layout_width="match_parent"
        android:layout_height="0.3dp"
        android:layout_marginBottom="5dp"
        android:background="#d7d7d7" />
    <android.support.v4.view.ViewPager
        android:id="@+id/vp_mycollect"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
