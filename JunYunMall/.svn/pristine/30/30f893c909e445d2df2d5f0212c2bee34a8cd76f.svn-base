package cn.junyun.mall.model;

public class MyFootPrintEntity {
	
	
	//访问日期
	private String VisitDate = 2016488+"";
	//商品图片
	private String GoodsPhotoUrl ="http://d01.res.meilishuo.net/pic/_o/eb/f8/037f5e7ff75a495c3e6c26d39371_850_850.c6.jpeg";
	//商品描述
	private String GoodsDescribe="eaweqr'jwqkl'rtweqtjret";
	//商品原价
	private double GoodsBeforePrice=98;
	//商品现价
	private double GoodsNowPrice=987;
	//判断是否是同一个日期
	private boolean sameDate = false;
	
	
	
	public boolean isSameDate() {
		return sameDate;
	}


	public void setSameDate(boolean sameDate) {
		this.sameDate = sameDate;
	}

	

	public MyFootPrintEntity() {
		super();
	}


	public MyFootPrintEntity(String visitDate, String goodsPhotoUrl,
			String goodsDescribe, double goodsBeforePrice, double goodsNowPrice) {
		super();
		VisitDate = visitDate;
		GoodsPhotoUrl = goodsPhotoUrl;
		GoodsDescribe = goodsDescribe;
		GoodsBeforePrice = goodsBeforePrice;
		GoodsNowPrice = goodsNowPrice;
	}
	
	



	public String getVisitDate() {
		return VisitDate;
	}


	public void setVisitDate(String visitDate) {
		VisitDate = visitDate;
	}


	public String getGoodsPhotoUrl() {
		return GoodsPhotoUrl;
	}


	public void setGoodsPhotoUrl(String goodsPhotoUrl) {
		GoodsPhotoUrl = goodsPhotoUrl;
	}


	public String getGoodsDescribe() {
		return GoodsDescribe;
	}


	public void setGoodsDescribe(String goodsDescribe) {
		GoodsDescribe = goodsDescribe;
	}


	public double getGoodsBeforePrice() {
		return GoodsBeforePrice;
	}


	public void setGoodsBeforePrice(double goodsBeforePrice) {
		GoodsBeforePrice = goodsBeforePrice;
	}


	public double getGoodsNowPrice() {
		return GoodsNowPrice;
	}


	public void setGoodsNowPrice(double goodsNowPrice) {
		GoodsNowPrice = goodsNowPrice;
	}


	@Override
	public String toString() {
		return "MyFootPrintEntity [VisitDate=" + VisitDate + ", GoodsPhotoUrl="
				+ GoodsPhotoUrl + ", GoodsDescribe=" + GoodsDescribe
				+ ", GoodsBeforePrice=" + GoodsBeforePrice + ", GoodsNowPrice="
				+ GoodsNowPrice + ", sameDate=" + sameDate + "]";
	}
	
	


}
