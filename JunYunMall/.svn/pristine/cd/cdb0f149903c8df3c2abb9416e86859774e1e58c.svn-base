<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".MyAchievementActivity" >

    <include
        android:id="@+id/title"
        layout="@layout/layout_header" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_below="@+id/title"
        android:background="@color/view_line_middle" />

    <RelativeLayout
        android:id="@+id/rlayout_mywallet"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_value_40"
        android:layout_below="@+id/view"
        android:background="@color/white"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/tv_account_money"
            style="@style/wrap_centerVertical"
            android:layout_marginLeft="16dp"
            android:text="我要推荐"
            android:textColor="@color/color_2b2b2b"
            android:textSize="@dimen/text_size_14" />

        <ImageView
            android:id="@+id/tv_mywallet_account_money"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@drawable/erweima" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/rlayout_mywallet"
        android:background="#e1e1e1" />

    <TableLayout
        android:id="@+id/MorePageTableLayout_Favorite"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rlayout_mywallet"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp" >

        <TableRow
            android:id="@+id/more_page_row0"
            android:layout_width="match_parent"
            android:layout_gravity="center_vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                android:text="我的成就"
                android:textColor="#ff333333"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:id="@+id/more_page_row1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#e1e1e1"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="55dp"
                android:layout_height="29dp"
                android:layout_margin="1dp"
                android:background="#ebebeb"
                android:gravity="center"
                android:text="层级"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />

            <TextView
                android:layout_width="135dp"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="1dp"
                android:layout_marginTop="1dp"
                android:background="#ebebeb"
                android:gravity="center"
                android:text="人数"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="#ebebeb"
                android:gravity="center"
                android:text="累计返利"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />
        </TableRow>

        <TableRow
            android:id="@+id/more_page_row2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#e1e1e1"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="55dp"
                android:layout_height="29dp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:background="#ffffff"
                android:gravity="center"
                android:text="1"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_peoplecount_firstfloor"
                android:layout_width="135dp"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:background="#ffffff"
                android:gravity="center"
                android:text="3人"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_rebate_firstfloor"
                android:layout_width="match_parent"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:background="#ffffff"
                android:gravity="center"
                android:text="￥0.30"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />
        </TableRow>

        <TableRow
            android:id="@+id/more_page_row3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#e1e1e1"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="55dp"
                android:layout_height="29dp"
                android:layout_margin="1dp"
                android:background="#ebebeb"
                android:gravity="center"
                android:text="2"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_peoplecount_secondfloor"
                android:layout_width="135dp"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="1dp"
                android:layout_marginTop="1dp"
                android:background="#ebebeb"
                android:gravity="center"
                android:text="7人"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_rebate_secondfloor"
                android:layout_width="match_parent"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="#ebebeb"
                android:gravity="center"
                android:text="￥0.50"
                android:textColor="#2b2b2b"
                android:textSize="12sp" />
        </TableRow>
    </TableLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="50dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="#969696" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center"
            android:text="查看详情请登录电脑版"
            android:textColor="#969696"
            android:textSize="12sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="#969696" />
    </LinearLayout>

</RelativeLayout>