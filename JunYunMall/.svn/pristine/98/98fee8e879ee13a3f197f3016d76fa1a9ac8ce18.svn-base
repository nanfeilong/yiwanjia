package cn.junyun.mall.model;
/**
 * 我的订单 内部listview实体类
 * @author Administrator
 *
 */
public class MyOrderAllInnerEntity {

	//商品图片
	private String GoodsPhotoUrl;
	//商品描述
	private String GoodsDescribe;
	//商品颜色分类
	private String GoodsColor;
	//商品尺码
	private String GoodsSize;
	//商品单价
	private double GoodsUnitPrice;
	//商品总数
	private int GoodsAllNumber;
	//交易金额
	private double TransactionAmount;
	//商品总价-退款金额
	private double GoodsAllPrice;



	public MyOrderAllInnerEntity(String goodsPhotoUrl, String goodsDescribe,
			String goodsColor, String goodsSize, double goodsUnitPrice,
			int goodsAllNumber, double goodsAllPrice) {
		super();
		GoodsPhotoUrl = goodsPhotoUrl;
		GoodsDescribe = goodsDescribe;
		GoodsColor = goodsColor;
		GoodsSize = goodsSize;
		GoodsUnitPrice = goodsUnitPrice;
		GoodsAllNumber = goodsAllNumber;
		GoodsAllPrice = goodsAllPrice;
	}





	public MyOrderAllInnerEntity(String goodsPhotoUrl, String goodsDescribe,
			String goodsColor, String goodsSize, double goodsUnitPrice,
			double transactionAmount, double goodsAllPrice) {
		super();
		GoodsPhotoUrl = goodsPhotoUrl;
		GoodsDescribe = goodsDescribe;
		GoodsColor = goodsColor;
		GoodsSize = goodsSize;
		GoodsUnitPrice = goodsUnitPrice;
		TransactionAmount = transactionAmount;
		GoodsAllPrice = goodsAllPrice;
	}





	public double getTransactionAmount() {
		return TransactionAmount;
	}


	public void setTransactionAmount(double transactionAmount) {
		TransactionAmount = transactionAmount;
	}


	public String getGoodsPhotoUrl() {
		return GoodsPhotoUrl;
	}
	public void setGoodsPhotoUrl(String goodsPhotoUrl) {
		GoodsPhotoUrl = goodsPhotoUrl;
	}
	public String getGoodsDescribe() {
		return GoodsDescribe;
	}
	public void setGoodsDescribe(String goodsDescribe) {
		GoodsDescribe = goodsDescribe;
	}
	public String getGoodsColor() {
		return GoodsColor;
	}
	public void setGoodsColor(String goodsColor) {
		GoodsColor = goodsColor;
	}
	public String getGoodsSize() {
		return GoodsSize;
	}
	public void setGoodsSize(String goodsSize) {
		GoodsSize = goodsSize;
	}
	public double getGoodsUnitPrice() {
		return GoodsUnitPrice;
	}
	public void setGoodsUnitPrice(double goodsUnitPrice) {
		GoodsUnitPrice = goodsUnitPrice;
	}
	public int getGoodsAllNumber() {
		return GoodsAllNumber;
	}
	public void setGoodsAllNumber(int goodsAllNumber) {
		GoodsAllNumber = goodsAllNumber;
	}
	public double getGoodsAllPrice() {
		return GoodsAllPrice;
	}
	public void setGoodsAllPrice(double goodsAllPrice) {
		GoodsAllPrice = goodsAllPrice;
	}





	@Override
	public String toString() {
		return "MyOrderAllInnerEntity [GoodsPhotoUrl=" + GoodsPhotoUrl
				+ ", GoodsDescribe=" + GoodsDescribe + ", GoodsColor="
				+ GoodsColor + ", GoodsSize=" + GoodsSize + ", GoodsUnitPrice="
				+ GoodsUnitPrice + ", GoodsAllNumber=" + GoodsAllNumber
				+ ", TransactionAmount=" + TransactionAmount
				+ ", GoodsAllPrice=" + GoodsAllPrice + "]";
	}


	


}
